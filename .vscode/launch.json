{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "SWA: Run angular-pokedex-main",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:4280",
      "preLaunchTask": "swa: start angular-pokedex-main",
      "webRoot": "${workspaceFolder}/",
      "timeout": 30000
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/wesle/OneDrive/Área de Trabalho/angular-pokedex/angular-pokedex-main",
      "program": "c:/Users/wesle/OneDrive/Área de Trabalho/angular-pokedex/angular-pokedex-main/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}